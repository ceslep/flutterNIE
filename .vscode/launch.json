  "version": "0.2.0",
  "configurations": [

    {
      "name": "Flutter ANDROID",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "deviceId": "2201117TL"
    },
    {
      "name": "Flutter",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--flavor", "dev"],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Flutter Attach",
      "request": "attach",
      "type": "dart",
      "deviceId": "flutter-tester",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "flutternie",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "flutternie (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "flutternie (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "Flutter (Release)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main.dart"
    },{
      "name": "Flutter Android Emulator",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "deviceId": "emulator-5554"
    }
  ]
}
